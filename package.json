{
  "name": "tre-svg-editor",
  "version": "1.0.0",
  "description": "SVG code editor with preview",
  "scripts": {
    "test": "tape test/**"
  },
  "keywords": [
    "tre",
    "ssb",
    "svg"
  ],
  "author": "Jan BÃ¶lsche <jan@lagomorph.de> (https://regular.codes/)",
  "license": "AGPL-3.0-or-later",
  "tre-import": {
    "messages": {
      "root": {
        "type": "folder",
        "name": "root"
      },
      "images": {
        "type": "folder",
        "name": "images",
        "root": "%root",
        "branch": "%root"
      },
      "heart-circle.svg": {
        "type": "svg",
        "name": "heart-circle",
        "svg": "$include assets/heart-circle.svg",
        "branch": "%images",
        "root": "%root"
      }
    }
  },
  "devDependencies": {
    "tape": "^4.9.2",
    "tre-client": "^4.1.0",
    "tre-editor-shell": "^1.5.0",
    "tre-finder": "^2.5.0",
    "tre-split-pane": "^1.0.1"
  },
  "dependencies": {
    "bl": "^2.1.2",
    "brace": "^0.11.1",
    "module-styles": "^1.0.0",
    "mutant": "^3.22.1",
    "pull-stream": "^3.6.9",
    "svgo": "^2.5.0",
    "tre-string": "^1.0.1"
  }
}
